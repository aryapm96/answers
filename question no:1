from array import *

def list_splitter(list_to_split, ratio):
    split = int(len(list_to_split) * ratio)
    return list_to_split[:split], list_to_split[split:]



def create(n):
    list_to_split=array('i',[]) 
    for i in range(n):    
        x=int(input("enter the element:"))
        list_to_split.append(x)
    print(list_to_split) 
    ratio=float(input("enter the ratio:"))
    return list_splitter(list_to_split, ratio)

n=int(input("Enter the range:"))
create(n)
